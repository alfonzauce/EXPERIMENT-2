//ROWS AND COLUMNS//



#include<iostream>


using namespace std;
 int main() 
{
    int row,column;
    
    cout << " Enter how many rows: "; cin >> row;
    cout << " Enter how many columns: "; cin >>column;
    
    for (int i=1; i<=row; i++)
    {
    	for (int j=1; j<=column; j++)
    	{
    		cout << "*";
		}
		cout<< "\n";
	}
return 0;
}
