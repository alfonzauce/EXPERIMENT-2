// LARGEST NUMBER//
#include<iostream>
using namespace std;

int main()
{
	int a,b,c;
	
	cout << "Enter the first number: "; cin >> a;
	cout << "Enter the second number: "; cin >> b;
	cout << "Enter the third number:  "; cin >> c;
	
	if (a>b && a>c )
	
		cout << "The Largest number between them is " << a; 
		
	else if (b>c && b>a)
	
		cout << " The Largest number between them is " << b; 
	
	else if ( b==a && b==c)
	
		cout << " The three numbers are equal ";
		
	else if ( b==a && b>c)
	
		cout << " The Largest number is " << b;
	
	else if ( b==c && b>a)
	
		cout << " The Largest number is " << b;
	
	else if ( c==a && c>b)
	
		cout << " The Largest number is " << c;
			
	else 
	
		cout << " The Largest number between them is " << c;  

return 0;
	
}
